{
  "expo": {
    "name": "rianmaps",
    "slug": "rianmaps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rm.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "package": "com.raven_ma.rianmaps"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
